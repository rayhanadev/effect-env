{
	"name": "@rayhanadev/env",
	"version": "1.0.0",
	"description": "Type-safe environment variable management for Effect. Like t3-env, but for Effect.",
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsdown",
		"lint": "biome check . --write --unsafe",
		"format": "biome format . --write",
		"typecheck": "tsc --noEmit",
		"prepublishOnly": "bun run build",
		"prepare": "effect-language-service patch"
	},
	"keywords": [
		"effect",
		"env",
		"environment",
		"config",
		"configuration",
		"type-safe",
		"typescript",
		"t3-env"
	],
	"author": "Rayhan Noufal Arayilakath <me@rayhanadev.com>",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/rayhanadev/effect-env.git"
	},
	"bugs": {
		"url": "https://github.com/rayhanadev/effect-env/issues"
	},
	"homepage": "https://github.com/rayhanadev/effect-env#readme",
	"dependencies": {},
	"peerDependencies": {
		"effect": ">=3.0.0",
		"typescript": ">=5.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.8",
		"@effect/language-service": "^0.59.0",
		"@types/bun": "latest",
		"effect": "^3.19.8",
		"tsdown": "^0.9.2"
	}
}
